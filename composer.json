{
    "name":        "arcanedev/laravel-html",
    "description": "HTML and Form Builders for the Laravel Framework",
    "keywords":    ["arcanedev", "arcanesoft", "laravel", "html", "form"],
    "homepage":    "https://github.com/ARCANEDEV/LaravelHtml",
    "authors": [
        {
            "name":     "ARCANEDEV",
            "email":    "arcanedev.maroc@gmail.com",
            "homepage": "https://github.com/ARCANEDEV"
        }
    ],
    "type":    "library",
    "license": "MIT",
    "require": {
        "php":                 ">=5.5.9",
        "arcanedev/support":   "~3.0",
        "illuminate/routing":  "~5.2.0",
        "illuminate/session":  "~5.2.0"
    },
    "require-dev": {
        "orchestra/testbench": "~3.2.0",
        "phpunit/phpcov":      "~2.0",
        "phpunit/phpunit":     "~4.0|~5.0"
    },
    "autoload": {
        "psr-4": {
            "Arcanedev\\LaravelHtml\\": "src/"
        },
        "files": ["helpers.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Arcanedev\\LaravelHtml\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.3-dev"
        }
    }
}
